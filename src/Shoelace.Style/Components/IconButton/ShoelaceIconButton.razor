@namespace Shoelace.Style.Components
@inherits ShoelaceComponentBase

<sl-icon-button @ref="Element"
                id="@Id"
                class="@Class"
                style="@Style"
                disabled="@Disabled"
                download="@Download"
                href="@Href"
                label="@Label"
                library="@Library"
                name="@Name"
                src="@Src"
                target="@Target"
                @onslblur="BlurHandlerAsync"
                @onslfocus="FocusHandlerAsync"
                @attributes="AdditionalAttributes"></sl-icon-button>